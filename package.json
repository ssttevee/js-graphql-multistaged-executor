{
  "name": "@ssttevee/graphql-multistage-executor",
  "version": "0.0.22",
  "license": "MIT",
  "main": "./lib.cjs",
  "module": "./lib.mjs",
  "types": "./lib.d.ts",
  "type": "module",
  "files": [
    "backends/*.cjs",
    "backends/*.mjs",
    "backends/*.d.ts",
    "*.cjs",
    "*.mjs",
    "*.d.ts"
