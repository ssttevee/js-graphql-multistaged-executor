    "build": "rollup -c",
    "pretest": "npm run clean",
    "test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "faunadb": "^4.8.0",
    "graphql": "^16.0.0",
    "jest": "^29.6.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "faunadb": "4.x",
    "graphql": "16.x"
  },
  "peerDependenciesMeta": {
    "faunadb": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssttevee/js-graphql-multistage-executor"
  }
